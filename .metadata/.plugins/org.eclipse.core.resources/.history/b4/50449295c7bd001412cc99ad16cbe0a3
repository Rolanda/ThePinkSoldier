import java.util.BitSet;


public class EssaiEntiers {

	public static void main(String[] args) {
		BitSet ensemble = new BitSet();
		ensemble.set(1);
		ensemble.set(4);
		ensemble.set(1000);
		System.out.println(ensemble.toString());
		ensemble.set(4);
		System.out.println(ensemble.toString());
		ensemble.clear(10);
		System.out.println(ensemble.toString());
		BitSet autreEnsemble = new BitSet();
		ensemble.set(1);
		ensemble.set(2);
		ensemble.set(1000);
		
		ensemble.or(autreEnsemble);
		System.out.println(ensemble.toString());
	}

}
