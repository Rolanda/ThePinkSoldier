
public class EssaiAnnee {

	public static void main(String[] args) {
		String[] mois = new String[]{"Janvier","Février","Mars","Avril","Mai","Juin",
				"Juillet", "Août", "Septembre", "Octobre", "Novembre", "Décembre"};
		String[] universitaire = new String[]{"Septembre", "Octobre", "Novembre",
				"Décembre","Janvier","Février","Mars","Avril","Mai","Juin"};
		String[] ete = new String[]{"Juin","Juillet","Août","Septembre"};
		Annee test = new Annee(mois);
		System.out.println(test);
		
		Annee uni = new Annee(universitaire);
		Annee chaud = new Annee(ete);
		
		System.out.println(uni);
		System.out.println(chaud);
		
		uni.union(uni);
		System.out.println(uni);
		uni.intersection(chaud);
	}

}
