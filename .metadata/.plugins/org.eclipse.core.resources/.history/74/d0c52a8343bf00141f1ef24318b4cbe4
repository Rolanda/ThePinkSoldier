
public class Multiplication<T> {
		T somme1;
		T somme2;
		
		public Multiplication(T somme1, T somme2) {
			this.somme1 = somme1;
			this.somme2 = somme2;
		}
		
		public Integer calculabiliterDesDeuxSommes(){
			Integer tmp = (Integer)this.somme1 * (Integer)this.somme2;
			return tmp;
		}
		
		public String toString(){
			return this.somme1 + " * " + this.somme2 + " = " + calculabiliterDesDeuxSommes();
		}
	}
}
