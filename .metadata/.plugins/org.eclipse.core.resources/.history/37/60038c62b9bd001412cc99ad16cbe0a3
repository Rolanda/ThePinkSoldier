import java.util.Map;


public class Dictionnaire {
	Map<String, String> dico;
	String[] fr;
	String[] eng;
	
	public Dictionnaire(/*HashMap<String[], String[]> dico*/String[] fr, String[] eng) {
		//this.fr = fr;
		//this.eng = eng;
		for (int i = 0; i < fr.length-1; i++) {
			dico.put(fr[i], eng[i]);
		}
	}
	
	public String toString(){
		return dico.toString();
	}
}
