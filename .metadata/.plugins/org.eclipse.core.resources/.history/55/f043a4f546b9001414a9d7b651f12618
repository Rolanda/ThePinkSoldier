import java.util.ArrayList;
import java.util.Random;
import java.util.Scanner;

import javax.swing.JFrame;
import javax.swing.JOptionPane;


public class Main {

	public static void main(String[] args) {
		// Initialisation de variable
		ArrayList<Integer> tableau = new ArrayList<Integer>();
		Random r = new Random();
		Scanner sc = new Scanner(System.in);

		if(JOptionPane.showConfirmDialog(null, "Voulez vous continuer ?") == 0){
		JOptionPane.showMessageDialog(null,"Donner une taille de tableau : ");
		int tailleTableau = sc.nextInt();
		int[][] tableauBis = new int[tailleTableau][tailleTableau];
		
		//remplissage
		for (int i = 0; i < tailleTableau; i++) {
			for (int j = 0; j < tailleTableau; j++) {
				tableauBis[j][i] = r.nextInt(10)+1;
				tableau.add(j, tableauBis[j][i]);
			}
			System.out.println();
		}
		JOptionPane.showMessageDialog(null,tableau + "\n" + tableau.size());
		}
	}

}
