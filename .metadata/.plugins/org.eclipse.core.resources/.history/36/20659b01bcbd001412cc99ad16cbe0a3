import java.util.HashSet;
import java.util.Iterator;
import java.util.Set;


public class Annee {
	Set<String> annee;
	String[] mois;
	
	public Annee() {
		String[] mois = new String[]{"Janvier","Février","Mars","Avril","Mai","Juin",
				"Juillet", "Août", "Septembre", "Octobre", "Novembre", "Décembre"};
		this.mois = mois;
		this.annee = new HashSet<String>();
		for (int i = 0; i < this.mois.length; i++) {
			this.annee.add(this.mois[i]);
		}
	}
	public Annee(String[] mois){
		this.annee = new HashSet<String>();
		this.mois = mois;
		for (int i = 0; i < mois.length; i++) {
			this.annee.add(mois[i]);
		}
	}
	
	public void union(Annee a){
		this.annee.add(a.mois));
	}
	
	public String toString(){
		String res = "";
		Iterator<String> it = annee.iterator();
		return "Il y a : "+this.annee.size() +", mois, qui sont : "+this.annee.toString();
	}
	
}
