import java.util.ArrayList;
import java.util.Random;
import java.util.Scanner;


public class Main {

	public static void main(String[] args) {
		// Initialisation de variable
		ArrayList<Integer> tableau = new ArrayList<Integer>();
		Random r = new Random();
		Scanner sc = new Scanner(System.in);
		System.out.println("Donner une taille de tableau : ");
		int tailleTableau = sc.nextInt();
		Integer[][] tableauBis = new Integer[tailleTableau][tailleTableau];
		
		//remplissage
		for (int i = 0; i < tailleTableau; i++) {
			for (int j = 0; j < tailleTableau; j++) {
				tableauBis[j][i] = r.nextInt(10);
			}
			tableau.add(i, tableauBis[i]);
		}

	}

}
