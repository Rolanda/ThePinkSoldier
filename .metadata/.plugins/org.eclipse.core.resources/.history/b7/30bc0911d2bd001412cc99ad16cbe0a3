import java.util.Map;
import java.util.Set;
import java.util.TreeMap;
import java.util.TreeSet;


public class Synonymes {
	
	Set<String> dicoSynonymes;
	
	public Synonymes(){
		this.dicoSynonymes = new TreeSet<String, String>();
	}
	
//	public Synonymes(String[] fr, String[] eng) {
//		this();
//		for (int i = 0; i < fr.length; i++) {
//			dicoSynonymes.put(fr[i], eng[i]);
//		}
//	}
	public void ajouterSynonymes(String mot, String[] synonymes){
		for (int i = 0; i < synonymes.length; i++) {
			if (!synonymes[i].equals(mot)) {
				dicoSynonymes.add(mot);
			}
		}
	}
}
