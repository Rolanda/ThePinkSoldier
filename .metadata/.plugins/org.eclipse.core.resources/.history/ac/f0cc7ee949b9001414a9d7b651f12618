import java.util.Arrays;


public class Plateau {
	private int taille;
	private char[][] table = new char[taille][taille];
	private boolean[][] tableBis = new boolean[taille][taille];
	private int indice;
	
	public int hashCode() {
		final int prime = 31;
		int result = 1;
		result = prime * result + Arrays.hashCode(table);
		return result;
	}
	public boolean equals(Object obj) {
		if (this == obj)
			return true;
		if (obj == null)
			return false;
		if (getClass() != obj.getClass())
			return false;
		Plateau other = (Plateau) obj;
		if (!Arrays.deepEquals(table, other.table))
			return false;
		return true;
	}

	public Plateau(char[][] table, boolean[][] tableBis, int indice) {
		for (int i = 0; i < tableBis.length; i++) {
			for (int j = 0; j < tableBis.length; j++) {
				this.table[j][i] = table[j][i];
				if(this.table[j][i] == 'x' ){
					this.tableBis = tableBis;
				}
			}
		}
	}
	

	/* (non-Javadoc)
	 * @see java.lang.Object#toString()
	 */
	@Override
	public String toString() {
		return "Plateau [taille=" + taille + ", table="
				+ Arrays.toString(table) + ", tableBis="
				+ Arrays.toString(tableBis) + ", indice=" + indice + "]";
	}
	
}
